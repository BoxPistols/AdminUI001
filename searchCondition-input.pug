  include pug/_sideBar
  // Content
  .main
    .hipsum
      .jumbotron
        .container.mt-2
          form.lay_form.row.px-4(action="searchCondition-result.html" method="post")
            .lay_title.mb-2.mt-2.d-flex.align-items-center
              h3.row.m_title.lv3.mt-4.ml-2.mb-3.d-flex.align-items-center
                span.ml-2.cl_green.mr-4
                  i(class="fas fa-search mr-2")
                  span IVF検索

            // /.lay_title
            .container.form-row.d-flex.justify-content-start                
              .formSection.d-md-flex
                .form-input.mr-4
                  label.form-label.mr-1(for='dev') 患者ID                    
                  input#dev.form-input(type='text', placeholder="P00001234")

                .form-input
                  label.form-label.mr-1(for='dev') IFVID：ART
                  input#dev2.form-input(type='text', placeholder="0056789")

              .formSection.d-flex.flex-nowrap(style='margin: 6px 0px 8px 16px')
                //- label.mr-sm-2(for='inlineFormCustomSelect2') 排卵時IVFID
                //- input(type='text', name='', value='')
                //- button#1.btn.btn-primary.idSearch(type='button', data-toggle='modal', data-target='#exampleModalCenter')
                //-   | 検索
            .m_hr.pt-1.pb-2(style='margin-top: 12px')
            .container.form-row

              .mr-3.mb-4
                h4.h4.div.d-flex
                  i.far.fa-calendar-alt.mr-2
                  .h5 実施日付

                .d-flex  
                  .form-check(style='padding:0')
                    input#defaultCheck1.form-check-input(type='checkbox', value='')
                    label.form-check-label(for='defaultCheck1', name="all")
                      | 全て
                  .custom-select-Group.input-group.mb-3
                    .input-group-prepend
                      label.input-group-text(for='inputGroupSelect01', name="year") 年
                    select#inputGroupSelect01.custom-select
                      option(selected='', value='') 年の選択
                      option(value='2019') 2019
                      //- option(value='2') 2020
                    .mr-4
                    .input-group-prepend
                      label.input-group-text(for='inputGroupSelect01', name="month") 月
                    select#inputGroupSelect01.custom-select
                      option(selected='', value='') 月の選択
                      option(value='9') 9
                      option(value='10') 10
                      option(value='11') 11
                      option(value='12') 12

                .form_SelectGroup      
                  h4.h4.div.d-flex.mt-4.mb-1
                    i.fas.fa-random.mr-2
                    .h5 移植方法1

                  .d-flex
                    .form-check.form-check-inline
                      input#inlineRadio1.form-check-input(type='radio', name='inlineRadioOptions', value='all' required, checked="checked")
                      label.form-check-label(for='inlineRadio1') 全て
                    .form-check.form-check-inline
                      input#inlineRadio2.form-check-input(type='radio', name='inlineRadioOptions', value='option2')
                      label.form-check-label(for='inlineRadio2') 新鮮胚移植
                    .form-check.form-check-inline
                      input#inlineRadio3.form-check-input(type='radio', name='inlineRadioOptions', value='option3')
                      label.form-check-label(for='inlineRadio3') 凍結胚移植
                    //- .form-check.form-check-inline
                    //-   input#inlineRadio4.form-check-input(type='radio', name='inlineRadioOptions', value='option4')
                    //-   label.form-check-label(for='inlineRadio4') 凍結胚移植

                .form_SelectGroup                
                  h4.h4.div.d-flex.mt-4.mb-1
                    i.fas.fa-random.mr-2
                    .h5 移植方法2

                  .d-flex.mb-3
                    .form-check.form-check-inline
                      input#inlineRadioDev2-1.form-check-input(type='radio', name='inlineRadioDev2-Options2', value='all', required, checked="checked")
                      label.form-check-label(for='inlineRadioDev2-1') 全て
                    .form-check.form-check-inline
                      input#inlineRadioDev2-2.form-check-input(type='radio', name='inlineRadioDev2-Options2', value='option2')
                      label.form-check-label(for='inlineRadioDev2-2') 単一胚移植
                    .form-check.form-check-inline
                      input#inlineRadioDev2-3.form-check-input(type='radio', name='inlineRadioDev2-Options2', value='option3')
                      label.form-check-label(for='inlineRadioDev2-3') 複数胚移植
                    .form-check.form-check-inline
                      input#inlineRadioDev2-4.form-check-input(type='radio', name='inlineRadioDev2-Options2', value='option4')
                      label.form-check-label(for='inlineRadioDev2-4') 二段階胚移植

                .form_SelectGroup
                  h4.h4.div.d-flex.mt-4.mb-1
                    i.fas.fa-edit.mr-2
                    .h5 入力状況

                  .d-flex
                    .form-check.form-check-inline
                      input#inlineRadioDev3-1.form-check-input(type='radio', name='inlineRadioDev3-Options3', value='all', required, checked="checked")
                      label.form-check-label(for='inlineRadioDev3-1') 全て
                    .form-check.form-check-inline
                      input#inlineRadioDev3-2.form-check-input(type='radio', name='inlineRadioDev3-Options3', value='option2')
                      label.form-check-label(for='inlineRadioDev3-2') 情報のみ
                    .form-check.form-check-inline
                      input#inlineRadioDev3-3.form-check-input(type='radio', name='inlineRadioDev3-Options3', value='option3')
                      label.form-check-label(for='inlineRadioDev3-3') 胚選択済
                    .form-check.form-check-inline
                      input#inlineRadioDev3-4.form-check-input(type='radio', name='inlineRadioDev3-Options3', value='option4')
                      label.form-check-label(for='inlineRadioDev3-4') 移植済
                    .form-check.form-check-inline
                      input#inlineRadioDev3-5.form-check-input(type='radio', name='inlineRadioDev3-Options3', value='option5')
                      label.form-check-label(for='inlineRadioDev3-5') 結果

                .form_SelectGroup
                  h4.h4.div.d-flex.mt-4.mb-1
                    i.fas.fa-poll.mr-2
                    .h5 結果状況

                  .d-flex
                    .form-check.form-check-inline
                      input#inlineRadioDev4-1.form-check-input(type='radio', name='inlineRadioDev4-Options4', value='all', required, checked="checked")
                      label.form-check-label(for='inlineRadioDev4-1') 全て
                    .form-check.form-check-inline
                      input#inlineRadioDev4-2.form-check-input(type='radio', name='inlineRadioDev4-Options4', value='option2')
                      label.form-check-label(for='inlineRadioDev4-2') 妊娠あり
                    .form-check.form-check-inline
                      input#inlineRadioDev4-3.form-check-input(type='radio', name='inlineRadioDev4-Options4', value='option3')
                      label.form-check-label(for='inlineRadioDev4-3') 出産あり
                    //- .form-check.form-check-inline
                    //-   input#inlineRadioDev4-4.form-check-input(type='radio', name='inlineRadioDev4-Options4', value='option4')
                    //-   label.form-check-label(for='inlineRadioDev4-4') 凍結胚移植

                //- label.mr-sm-2.ml-1(for='') 実施日付
                //- selsect
                //-   option(value="1")  

            .container.mt-4

            .container.mt-4
              .row
                //- a(href='searchCondition-result.html')
                a
                  button.btn.btn-primary(type='submit', value='検索する', style='width:240px; margin: 10px auto 20px 190px; padding: .8em 2em')
                    | IVF検索

